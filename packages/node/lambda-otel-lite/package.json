{
  "name": "@dev7a/lambda-otel-lite",
  "version": "0.6.0",
  "description": "Lightweight OpenTelemetry instrumentation for AWS Lambda",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./extension": {
      "types": "./dist/extension/index.d.ts",
      "default": "./dist/extension/index.js"
    },
    "./telemetry": {
      "types": "./dist/telemetry/index.d.ts",
      "default": "./dist/telemetry/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "clean": "rm -rf dist",
    "test": "jest -c jest.config.ts",
    "test:watch": "jest -c jest.config.ts --watch",
    "test:coverage": "jest -c jest.config.ts --coverage",
    "lint": "eslint 'src/**/*.{js,ts}' '__tests__/**/*.{js,ts}' --fix"
  },
  "dependencies": {
    "@dev7a/otlp-stdout-span-exporter": "^0.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^1.19.0",
    "@opentelemetry/resources": "^1.19.0",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.19.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  }
}
